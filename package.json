{
  "name": "@zk-email/sdk",
  "version": "2.0.1",
  "description": "ZK Email SDK for TypeScript",
  "main": "./dist/zk-email-sdk.index.cjs",
  "module": "./dist/zk-email-sdk.index.mjs",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zkemail/zk-email-sdk-js.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/zk-email-sdk.index.mjs",
      "require": "./dist/zk-email-sdk.index.cjs"
    },
    "./initNoirWasm": {
      "types": "./dist/types/initNoirWasm.d.ts",
      "import": "./dist/zk-email-sdk.initNoirWasm.mjs",
      "require": "./dist/zk-email-sdk.initNoirWasm.cjs"
    }
  },
  "files": [
    "dist",
    "NPM-README.md",
    "assets"
  ],
  "scripts": {
    "build-old": "bunx tsup src/index.ts src/localProverWorker.ts --format esm,cjs --dts",
    "build": "vite build",
    "build-prove-worker": "bun x vite build --config vite.config.worker.ts && bun scripts/escape_str_chars.js",
    "publish": "bun run build && npm publish --access public",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "publish-nightly": "bun run build && npm publish --access public --tag nightly",
    "release:patch": "npm version patch && bun run release",
    "release:minor": "npm version minor && bun run release",
    "release:major": "npm version major && bun run release",
    "release": "bun run publish && git push && git push --tags && gh release create v$npm_package_version --generate-notes",
    "changelog": "echo 'Please update CHANGELOG.md manually before releasing'"
  },
  "keywords": [
    "zk",
    "email",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@aztec/bb.js": "0.82.2",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@noir-lang/noir_js": "1.0.0-beta.6",
    "@noir-lang/noirc_abi": "1.0.0-beta.6",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/bun": "^1.1.12",
    "@types/google.accounts": "^0.0.17",
    "@types/pg": "^8.11.10",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "localforage": "^1.10.0",
    "localforage-memoryStorageDriver": "^0.9.2",
    "open": "^10.1.0",
    "pako": "^2.1.0",
    "pg": "^8.13.0",
    "prettier": "^3.3.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "vite": "^6.0.7",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-replace": "^0.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.5.1",
    "@mach-34/noir-bignum-paramgen": "^1.1.2",
    "@peculiar/webcrypto": "^1.5.0",
    "@zk-email/relayer-utils": "0.4.66-nightly.20250523-1",
    "@zk-email/snarkjs": "^0.0.1",
    "@zk-email/zkemail-nr": "1.3.3-3",
    "ethers": "^6.13.4",
    "jszip": "^3.10.1",
    "poseidon-lite": "^0.3.0",
    "rsa-key": "^0.0.6",
    "snarkjs": "^0.7.5",
    "viem": "^2.21.53",
    "zod": "^3.23.8"
  }
}
